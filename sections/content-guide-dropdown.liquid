                                  {%- endif %}
                                  <div>
                                    {% if block.settings.serum_yellow_tag != blank %}
                                      <h5 class="bg-yellow">{{ block.settings.serum_yellow_tag }}</h5>
                                    {% endif %}
                                    {% if block.settings.serum_yellow_heading != blank %}
                                      <p>{{ block.settings.serum_yellow_heading }}</p>
                                    {% endif %}
                                  </div>
                                </div>
                                {% if block.settings.serum_yellow_info != blank %}
                                  {{ block.settings.serum_yellow_info }}
                                {% endif %}
                              {% endif %}
                              {% if block.settings.serum_grey_heading != blank %}
                                <div class="serums_wrap">
                                  {% if block.settings.serum_grey_img != blank %}
                                    <img
                                      src="{{block.settings.serum_grey_img | img_url:'master'}}"
                                      alt=""
                                      class="img-fluid"
                                    >
                                  {%- endif %}
                                  <div>
                                    {% if block.settings.serum_grey_tag != blank %}
                                      <h5 class="bg-grey">{{ block.settings.serum_grey_tag }}</h5>
                                    {% endif %}
                                    {% if block.settings.serum_grey_heading != blank %}
                                      <p>{{ block.settings.serum_grey_heading }}</p>
                                    {% endif %}
                                  </div>
                                </div>
                              {% endif %}
                              {% if block.settings.serum_grey_info != blank %}
                                {{ block.settings.serum_grey_info }}
                              {% endif %}
                            </div>
                          </div>
                        </div>
                      {% endif %}
                    </div>
                  </div>
                  <div class="ac_desc">
                    <div class="as_desc_header">
                      {% if block.settings.heading2_icon != blank %}
                        <img
                          src="{{ block.settings.heading2_icon | img_url:'master'}}"
                          alt="block.settings.heading2_icon.alt"
                          class="img-fluid me-2"
                        >
                      {% endif %}
                      {% if block.settings.bl2_heading != blank %}
                        {{ block.settings.bl2_heading }}
                      {% endif %}
                    </div>
                    {% if block.settings.bl2_list != blank %}
                      {{ block.settings.bl2_list }}
                    {% endif %}
                  </div>
                  <div class="ac_desc">
                    <div class="as_desc_header">
                      {% if block.settings.heading3_icon != blank %}
                        <img
                          src="{{ block.settings.heading3_icon | img_url:'master'}}"
