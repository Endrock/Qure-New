                                  {{- block.settings.dropdown1_heading -}}
                                {%- endif %}
                              </div>
                            </button>
                          </h2>
                          <div
                            id="asCollapseOne"
                            class="accordion-collapse collapse show"
                            aria-labelledby="asHeaderOne"
                            data-bs-parent="#as_dropdown"
                          >
                            <div class="accordion-body">
                              {% if block.settings.dropdown1_content != blank -%}
                                {{ block.settings.dropdown1_content }}
                              {%- endif %}
                            </div>
                          </div>
                        </div>
                      {% endif %}
                      {% if block.settings.dropdown2_heading != blank %}
