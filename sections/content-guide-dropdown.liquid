                            <div class="accordion-body">
                              {% if block.settings.dropdown2_content != blank -%}
                                {{ block.settings.dropdown2_content }}
                              {%- endif %}
                            </div>
                          </div>
                        </div>
                      {% endif %}
                      {% if block.settings.dropdown3_heading != blank -%}
                        <div class="accordion-item">
                          <h2 class="accordion-header" id="asheadingThree">
                            <button
                              class="accordion-button collapsed"
                              type="button"
                              data-bs-toggle="collapse"
                              data-bs-target="#ascollapseThree"
                              aria-expanded="false"
                              aria-controls="ascollapseThree"
                            >
                              <div class="container">
                                {% if block.settings.dropdown3_heading != blank -%}
                                  {{- block.settings.dropdown3_heading -}}
                                {%- endif %}
                              </div>
                            </button>
                          </h2>
                          <div
                            id="ascollapseThree"
                            class="accordion-collapse collapse"
                            aria-labelledby="asheadingThree"
                            data-bs-parent="#as_dropdown"
                          >
                            <div class="accordion-body">
                              {% if block.settings.dropdown3_content != blank -%}
                                {{ block.settings.dropdown3_content }}
                              {%- endif %}
                              {% if block.settings.serum_yellow_heading != blank %}
                                <div class="serums_wrap">
                                  {% if block.settings.serum_yellow_img != blank %}
                                    <img
                                      src="{{block.settings.serum_yellow_img | img_url:'master'}}"
