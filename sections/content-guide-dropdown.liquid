                      {% endif %}
                      {% if block.settings.bl3_heading != blank %}
                        {{ block.settings.bl3_heading }}
                      {% endif %}
                    </div>
                    {% if block.settings.bl3_list != blank %}
                      {{ block.settings.bl3_list }}
                    {% endif %}
                  </div>
                </div>
              </div>
            {% when 'product_information' %}
              <div
                class="tab-pane"
                id="{{block.id}}"
                role="tabpanel"
                aria-labelledby="{{block.id}}-tab"
              >
                <div class="ac_desc_grid">
                  <div class="ac_desc">
                    <div class="as_desc_header">
