<!-- -- amazing content start -- -->
<div class="amazing_content section" id="tabsAccordion">
  <div class="container">
    <div class="section_header mw-680 text-md-center mb-3">
      {% if section.settings.heading != blank %}
        <h2 class="title-lg">{{ section.settings.heading }}</h2>
      {% endif %}
    </div>
    <div class="amazing_content_wrap d-none d-md-block">
      <ul class="nav nav-tabs" id="myTab" role="tablist">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'content_requirements' %}
              <li class="nav-item" role="presentation">
                {% if block.settings.step != blank %}
                  <h6>{{ block.settings.step }}</h6>
                {% endif %}
                {% if block.settings.heading != blank %}{{ block.settings.heading }}{% endif %}
                {% if block.settings.image != blank -%}
                  <img
                    src="{{ block.settings.image | img_url:'master' }}"
                    alt="{{ block.settings.image.alt }}"
                    class="img-fluid mt-2"
                  >
                {%- endif %}
                {% if block.settings.button_label != blank %}
                  <button
                    class="nav-link active ac_drop_btn"
                    id="{{block.id}}-tab"
                    data-bs-toggle="tab"
                    data-bs-target="#{{block.id}}"
                    type="button"
                    role="tab"
                    aria-controls="{{block.id}}"
                    aria-selected="true"
                  >
                    <div class="container">
                      <p>{{ block.settings.button_label }}</p>
                    </div>
                  </button>
                {% endif %}
              </li>
            {% when 'product_information' %}
              <li class="nav-item" role="presentation">
                {% if block.settings.step != blank %}
                  <h6>{{ block.settings.step }}</h6>
                {% endif %}
                {% if block.settings.heading != blank %}{{ block.settings.heading }}{% endif %}
                {% if block.settings.image != blank -%}
                  <img
                    src="{{ block.settings.image | img_url:'master' }}"
                    alt="{{ block.settings.image.alt }}"
                    class="img-fluid mt-2"
                  >
                {%- endif %}
                {% if block.settings.button_label != blank %}
                  <button
                    class="nav-link active ac_drop_btn"
                    id="{{block.id}}-tab"
                    data-bs-toggle="tab"
                    data-bs-target="#{{block.id}}"
                    type="button"
                    role="tab"
                    aria-controls="{{block.id}}"
                    aria-selected="true"
                  >
                    <div class="container">
                      <p>{{ block.settings.button_label }}</p>
                    </div>
                  </button>
                {% endif %}
              </li>
            {% when 'inspiring_content' %}
              <li class="nav-item" role="presentation">
                {% if block.settings.step != blank %}
                  <h6>{{ block.settings.step }}</h6>
                {% endif %}
                {% if block.settings.heading != blank %}{{ block.settings.heading }}{% endif %}
                {% if block.settings.image != blank -%}
                  <img
                    src="{{ block.settings.image | img_url:'master' }}"
                    alt="{{ block.settings.image.alt }}"
                    class="img-fluid mt-2"
                  >
                {%- endif %}
                {% if block.settings.button_label != blank %}
                  <button
                    class="nav-link active ac_drop_btn"
                    id="{{block.id}}-tab"
                    data-bs-toggle="tab"
                    data-bs-target="#{{block.id}}"
                    type="button"
                    role="tab"
                    aria-controls="{{block.id}}"
                    aria-selected="true"
                  >
                    <div class="container">
                      <p>{{ block.settings.button_label }}</p>
                    </div>
                  </button>
                {% endif %}
              </li>
          {% endcase %}
        {% endfor %}
      </ul>
      <div class="tab-content" id="myTabContent">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'content_requirements' %}
              <div
                class="tab-pane {% if forloop.first %} active show {%endif %}"
                id="{{block.id}}"
                role="tabpanel"
                aria-labelledby="{{block.id}}-tab"
              >
                <div class="ac_desc_grid">
                  <div class="ac_desc faq">
                    <div class="as_desc_header">
                      {% if block.settings.heading1_icon != blank %}
                        <img
                          src="{{ block.settings.heading1_icon | img_url:'master'}}"
                          alt="block.settings.heading1_icon.alt"
                          class="img-fluid me-2"
                        >
                      {% endif %}
                      {% if block.settings.bl1_heading != blank %}
                        {{ block.settings.bl1_heading }}
                      {% endif %}
                    </div>
                    {% if block.settings.bl1_subheading != blank %}
                      <p>{{ block.settings.bl1_subheading }}</p>
                    {% endif %}

                    <div class="accordion" id="as_dropdown">
                      {% if block.settings.dropdown1_heading != blank %}
                        <div class="accordion-item">
                          <h2 class="accordion-header" id="asHeaderOne">
                            <button
                              class="accordion-button"
                              type="button"
                              data-bs-toggle="collapse"
                              data-bs-target="#asCollapseOne"
                              aria-expanded="true"
                              aria-controls="asCollapseOne"
