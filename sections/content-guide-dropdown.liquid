<script>
  // for making the amazing content inactive all tabs by default
  document.addEventListener('DOMContentLoaded', function () {
    // Ensure no tab is active
    document.querySelectorAll('#myTab .nav-link').forEach(function (tab) {
      tab.classList.remove('active');
      tab.setAttribute('aria-selected', 'false');
    });

    // Ensure no tab content is active
    document.querySelectorAll('#myTabContent .tab-pane').forEach(function (content) {
      content.classList.remove('show', 'active');
    });
  });
</script>
